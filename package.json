{
	"name": "laf",
	"version": "0.2.1",
	"main": "src/index.js",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/servexyz/lingo-asset-fetcher-cli"
	},
	"devDependencies": {
		"@ava/babel-preset-stage-4": "^2.0.0",
		"@babel/core": "7.4.3",
		"@babel/node": "^7.2.2",
		"@babel/polyfill": "^7.4.3",
		"@babel/preset-env": "7.4.3",
		"@babel/preset-react": "^7.0.0",
		"@babel/register": "7.4.0",
		"@packtracker/webpack-plugin": "^2.0.1",
		"ava": "^1.4.1",
		"babel-loader": "^8.0.5",
		"duplicate-package-checker-webpack-plugin": "^3.0.0",
		"nodemon": "^1.18.10",
		"nodemon-webpack-plugin": "^4.0.8",
		"npm-run-all": "^4.1.5",
		"npm-starter-sample-module": "0.0.4",
		"size-plugin": "^1.2.0",
		"source-map-loader": "^0.2.4",
		"webpack": "^4.29.6",
		"webpack-cli": "^3.3.0",
		"webpack-node-externals": "^1.7.2"
	},
	"dependencies": {
		"dotenv": "^6.1.0",
		"fs-extra": "^7.0.1",
		"laf-lib": "^0.2.6",
		"meow": "^5.0.0"
	},
	"scripts": {
		"start": "npm run dev:meow:node",
		"clean": "rm -Rf ./build",
		"dev": "npm run dev:vanillaModule",
		"dev:meow:gen": "babel-node src/index.js gen",
		"dev:meow:node": "./src/index.js gen",
		"dev:vanillaModule": "babel-node src/index.js",
		"dev:liveReloadedModule": "nodemon --no-stdin --watch src/index.js --exec 'babel-node src/index.js'",
		"dev:builtModule": "npm run build && node build/main.js",
		"test": "npm-run-all -s clean:build build test:liveReloadedAva",
		"test:ava": "ava",
		"test:liveReloadedAva": "ava --watch",
		"build": "webpack --mode production --display minimal"
	},
	"ava": {
		"files": [
			"!npm-starter-sample-module/**/*.js",
			"tests/"
		],
		"require": [
			"@babel/register",
			"@babel/polyfill"
		],
		"verbose": true
	}
}
